# mount point		fstype		device				 	[flags]  	
/recovery			emmc		/dev/block/platform/sprd-sdhci.3/by-name/RECOVERY 	flags=backup=1;display=Recovery
/boot				emmc		/dev/block/platform/sprd-sdhci.3/by-name/KERNEL	
/cache				ext4		/dev/block/platform/sprd-sdhci.3/by-name/CSC 	
/data				ext4		/dev/block/platform/sprd-sdhci.3/by-name/userdata	length=-16384		
/system				ext4		/dev/block/platform/sprd-sdhci.3/by-name/system
/external_sd		vfat		/dev/block/mmcblk1p1	flags=display="Micro SDcard";storage;wipeingui;removable
